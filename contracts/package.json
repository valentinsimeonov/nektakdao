    "@nomiclabs/hardhat-ethers": "2.2.3",
    "@nomiclabs/hardhat-etherscan": "3.1.8",
    "@typechain/ethers-v5": "10.2.0",
    "@typechain/hardhat": "6.1.6",
    "chai": "4.3.7",
    "dotenv": "16.3.1",
    "ethers": "5.7.2",
    "hardhat": "2.16.0",
    "ts-node": "10.9.1",
    "typechain": "8.3.2",
    "typescript": "5.2.2"
